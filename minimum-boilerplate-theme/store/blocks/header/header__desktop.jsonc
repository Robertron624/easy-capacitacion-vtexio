      "login": {
        "props": {
            "showIconProfile": true,
            "iconLabel": "Mi cuenta"
        }
      },
    
      "flex-layout.row#content-header": {
        "props": {
          "horizontalAlign": "center",
          "verticalAlign": "center",
          "preventHorizontalStretch": true,
          "preventVerticalStretch": true,
          "fullWidth": true,
          "colSizing": "equal"
        },
        "children": [
          "flex-layout.col#logo-desktop",
          "search-bar",
          "flex-layout.col#spacer",
          "minicart.v2"
        ]
      },

      "flex-layout.col#spacer": {
        "props": {
          "width": "grow"
        }
      },

      "flex-layout.col#logo-desktop": {
        "props": {
          "verticalAlign": "middle"
        },
        "children": ["logo"]
      },

      "logo": {
        "props": {
          "title": "Logo",
          "href": "/",
          "url": "assets/header/logo-easy.webp",
          "width": "100"
        }
      },

      "search-bar": {
        "props": {
          "placeholder": "Busca tu producto"
        }
      },

      "flex-layout.row#menus-header":{
        "title": "Menus header",
        "props": {
          "fullWidth": true,
          "blockClass": "menus-header",
          "colSizing": "auto"
        },
        "children": [
          "flex-layout.col#left-menus",
          "flex-layout.col#right-menus"
        ]
      },

      "flex-layout.col#left-menus": {
        "children": [
          "flex-layout.row#left-menus"
        ],
        "props": {
          "blockClass": [
            "left-menu"
          ]
        }
      },

      "flex-layout.row#left-menus": {
        "props": {
          "fullWidth": true
        },
        "children": [
          "menu#categorias",
          "rich-text#link-easy",
          "rich-text#link-tc-cencosud",
          "rich-text#link-catalog"

        ]
      },

      "rich-text#link-easy": {
        "props": {
          "text": "![easy](assets/header/icon-blog.png)[Hazlo fácil, Hazlo easy](/#)",
          "blockClass":[
            "menu-link"
          ]
        }
      },
      
      "rich-text#link-tc-cencosud": {
        "props": {
          "text": "[Tarjeta Cencosud](/#)",
          "blockClass":[
            "menu-link"
          ]
        }
      },

      "rich-text#link-catalog": {
        "props": {
          "text": "![catalogo](assets/header/icon-catalogo.png)[Catálogo](/#)",
          "blockClass":[
            "menu-link"
          ]
        }
      },

      "flex-layout.col#right-menus": {
        "props": {
          "blockClass":[
            "right-menu"
          ]
        },
        "children": [
          "flex-layout.row#right-menus"
        ]
      },

      "flex-layout.row#right-menus": {
        "props": {
          "fullWidth": true,
          "colSizing": "auto"
        },
        "children": [
          "image#bio",
          "image#services"
        ]
      },

      "image#bio": {
        "props": {
          "src": "assets/header/bioseguridad.webp",
          "link": {
            "openNewTab": true,
            "url": "/biosecurity",
            "title": "bioseguridad"
          }
        }
      },

      "image#services": {
        "props": {
          "src": "assets/header/instalaciones.webp",
          "link": {
            "openNewTab": true,
            "url": "/services",
            "title": "bioseguridad"
          }
        }
      }
}