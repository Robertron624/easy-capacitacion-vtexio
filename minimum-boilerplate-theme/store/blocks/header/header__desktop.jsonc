{
    "header-layout.desktop": {
        "children": [
          "flex-layout.row#top-banner",
          "flex-layout.row#top-header",
          "flex-layout.row#content-header",
          "flex-layout.row#menus-header"
        ]
      },

      "flex-layout.row#top-banner": {
        "title": "Top banner",
        "children": [
          "slider-layout#top-banner"
        ],
        "props": {
          "fullWidth": true,
          "blockClass": ["top-banner"]
        }
      },
    
      "slider-layout#top-banner": {
        "title": "Top banner slider",
        "props": {
          "fullWidth": true,
          "blockClass":["top-banner"],
          "showNavigationArrows": "never",
          "showPaginationDots": "never",
          "infinite": true,
          "autoplay": {
            "stopOnHover": true,
            "timeout": 4000
          },
          "itemsPerPage": {
            "desktop": 1,
            "tablet": 1,
            "phone": 1
          }
        },
        "children": [
          "image#top-banner-1"
        ]
      },
    
    
      "image#top-banner-1":{
        "title": "Top banner image",
        "props": {
          "src": "assets/header/banner-superior-venta-telefonica.webp"
        }
      },
  
      "rich-text": { 
        "props": { 
          "text": "VTEX Store Framework",
          "textAlignment": "CENTER",
          "textPosition": "CENTER",
          "font": "t-heading-1"
        }
      },

      "flex-layout.row#top-header": {
        "title": "Top header row",
        "props": {
            "blockClass": "top-header",
            "fullWidth": true,
            "colSizing": "auto"
        },
        "children": [
            "login"
        ]
      },

