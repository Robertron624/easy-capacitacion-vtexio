{
  "store.home": { 
    "blocks": [
      "flex-layout.row#slider__home",
      "flex-layout.row#promotions__1",
      "flex-layout.row#combos",
      "flex-layout.row#promotions__2"
    ]
  },

  "flex-layout.row#promotions__1":{
    "props": {
      "blockClass": "home-promos",
      "fullWidth": true,
      "colSizing": "auto"
    },
    "children": [
      "flex-layout.col#promotions__1"
    ]
  },

  "flex-layout.col#promotions__1":{
    "props": {
      "blockClass":[
        "home-promos"
      ]
    },
    "children": [
      "rich-text#promo__title--1",
      "flex-layout.row#promos__images--1"
    ]
  },
  
  "rich-text#promo__title--1": {
    "title": "promo title",
    "props": {
      "text": "Renueva MÁS por MENOS",
      "textAlignment": "CENTER",
      "textPosition": "LEFT",
      "blockClass":[
        "promo-title"
      ]
    }
  },

  "flex-layout.row#promos__images--1": {
    "props": {
      "fullWidth": true,
      "colSizing": "auto"
    },
    "children": [
      "image#promo__1--1",
      "image#promo__1--2",
      "image#promo__1--3"
    ]
  },

  "image#promo__1--1": {
    "props": {
      "blockClass":[
        "home-promo"
      ],
