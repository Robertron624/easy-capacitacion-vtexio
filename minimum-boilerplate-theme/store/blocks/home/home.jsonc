  "flex-layout.row#combos":{
    "props": {
      "blockClass": "home-combos",
      "fullWidth": true,
      "colSizing": "auto"
    },
    "children": [
      "flex-layout.col#combo__shelf"
    ]
  },

  "flex-layout.col#combo__shelf": {
    "props": {
      "blockClass":[
        "shelf-combo"
      ]
    },
    "children": [
      "rich-text#combo__title",
      "image#combo__1",
      "list-context.product-list#home-combos"
    ]
  },

  "rich-text#combo__title": {
    "title": "combo title",
    "props": {
      "text": "Renueva MÁS con estos COMBOS",
      "textAlignment": "CENTER",
      "textPosition": "LEFT",
